{
    "name": "jtl/shop5-router-lib",
    "description": "JTL-Shop5 Router library",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Daniel BoÌˆhmer",
        "email": "daniel.boehmer@jtl-software.com"
    }],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/http-foundation": "^4.3.2",
        "izzle/model": "^0.9.0",
        "izzle/translation": "^0.1.0",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "JTL\\Shop5Router\\": "src/"
        }
    },
    "scripts": {
        "phpcs": "vendor/bin/php-cs-fixer fix --config=./.php_cs.dist",
        "phpcs-dry": "vendor/bin/php-cs-fixer fix --config=./.php_cs.dist -v --dry-run --using-cache=no"
    }
}
